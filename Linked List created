package Myclass;

class linked_list{
	
	int data;
	linked_list next;
	linked_list(int x){
		data = x;
		next = null;
	}
}
public class Implementing_LL {

	public static void main(String[] args) {
	
		linked_list head = new linked_list(10);
		linked_list temp1 = new linked_list(20);
		linked_list temp2 = new linked_list(30);
		head.next = temp1;
		temp1.next = temp2;
		temp2.next = null;
		System.out.println("Linked List Created");
	}

}
